- content_tag_for :div, report do
  %ul
    %li
      %strong Yql:
      =h report.yql_id
    %li
      %strong Provenance:
      =h report.provenance
    %li
      %strong Content:
      =h report.content
    %li
      %strong Latitude:
      =h report.latitude
    %li
      %strong Longitude:
      =h report.longitude
    %li
      %strong Votes:
      =h report.votes
    = link_to 'Show', report
    = link_to 'Edit', edit_report_path(report)
    = link_to 'Destroy', report, :confirm => 'Are you sure?', :method => :delete